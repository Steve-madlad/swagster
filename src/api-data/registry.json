    {
      "id": "notification-api",
      "name": "Notification Center",
      "description": "A centralized gateway for multi-channel user communication. It abstracts the complexity of SMTP and SMS providers, allowing for templated notifications that support dynamic variable injection. It ensures that critical alerts, like renewal reminders or payment failures, reach users via their preferred channels.",
      "version": "1.0",
      "baseUrl": "https://notification-center.example.com/api/v1",
      "resources": [
        {
          "groupName": "Notifications",
          "endpoints": [
            {
              "description": "Send Email Notification",
              "path": "/notifications/email",
              "method": "POST",
              "authenticated": true,
              "request": {
                "queryParams": null,
                "headers": null,
                "body": {
                  "to": {
                    "required": true,
                    "type": "string",
                    "description": "Recipient email address"
                  },
                  "subject": {
                    "required": true,
                    "type": "string",
                    "description": "Email subject"
                  },
                  "templateId": {
                    "required": false,
                    "type": "string",
                    "description": "Template identifier to render"
                  },
                  "variables": {
                    "required": false,
                    "type": "object",
                    "description": "Key-value pairs for template variables"
                  }
                }
              },
              "responseSample": {
                "success": true,
                "message": "email queued",
                "data": {
                  "notificationId": "ntf_123",
                  "channel": "email",
                  "status": "queued"
                }
              }
            },
            {
              "description": "Send SMS Notification",
              "path": "/notifications/sms",
              "method": "POST",
              "authenticated": true,
              "request": {
                "queryParams": null,
                "headers": null,
                "body": {
                  "to": {
                    "required": true,
                    "type": "string",
                    "description": "Recipient phone number in E.164 format"
                  },
                  "message": {
                    "required": true,
                    "type": "string",
                    "description": "Text message content"
                  }
                }
              },
              "responseSample": {
                "success": true,
                "message": "sms queued",
                "data": {
                  "notificationId": "ntf_456",
                  "channel": "sms",
                  "status": "queued"
                }
              }
            }
          ]
        }
      ],
      "authentication": {
        "type": "API_KEY",
        "headerName": "X-Api-Key"
      }
    },
    {
      "id": "analytics-api",
      "name": "Event Analytics",
      "description": "An event-driven data ingestion and querying engine. This service captures granular user interactions and system events, providing a high-throughput path for real-time tracking. It allows developers and data analysts to query historical trends to optimize user retention and platform performance.",
      "version": "1.0",
      "baseUrl": "https://analytics.example.com/api/v1",
      "resources": [
        {
          "groupName": "Events",
          "endpoints": [
            {
              "description": "Ingest Event",
              "path": "/events",
              "method": "POST",
              "authenticated": false,
              "request": {
                "queryParams": null,
                "headers": null,
                "body": {
                  "type": {
                    "required": true,
                    "type": "string",
                    "description": "The event type identifier (e.g., page_view)"
                  },
                  "userId": {
                    "required": false,
                    "type": "string",
                    "description": "The user identifier"
                  },
                  "properties": {
                    "required": false,
                    "type": "object",
                    "description": "Additional event properties/metadata"
                  },
                  "timestamp": {
                    "required": false,
                    "type": "string",
                    "description": "ISO8601 timestamp; defaults to now"
                  }
                }
              },
              "responseSample": {
                "success": true,
                "data": {
                  "eventId": "evt_123",
                  "receivedAt": "2026-02-13T09:45:00.000Z"
                }
              }
            },
            {
              "description": "Query Events",
              "path": "/events",
              "method": "GET",
              "authenticated": true,
              "request": {
                "queryParams": {
                  "type": {
                    "description": "Filter by event type",
                    "required": false
                  },
                  "from": {
                    "description": "Start of time range (ISO8601)",
                    "required": false
                  },
                  "to": {
                    "description": "End of time range (ISO8601)",
                    "required": false
                  }
                },
                "headers": null,
                "body": null
              },
              "responseSample": {
                "success": true,
                "data": [
                  {
                    "id": "evt_123",
                    "type": "page_view",
                    "userId": "user_1",
                    "properties": {
                      "path": "/pricing"
                    },
                    "timestamp": "2026-02-13T09:45:00.000Z"
                  }
                ]
              }
            }
          ]
        }
      ],
      "authentication": {
        "type": "Bearer",
        "headerName": "Authorization"
      }
